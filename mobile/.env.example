# API Configuration
API_BASE_URL=http://localhost:3000/api
API_TIMEOUT=5000

# Google Maps Configuration
GOOGLE_MAPS_API_KEY=votre_google_maps_api_key
GOOGLE_PLACES_API_KEY=votre_google_places_api_key

# Firebase Configuration
FIREBASE_API_KEY=votre_firebase_api_key
FIREBASE_AUTH_DOMAIN=votre_projet.firebaseapp.com
FIREBASE_PROJECT_ID=votre_projet_id
FIREBASE_STORAGE_BUCKET=votre_bucket.appspot.com
FIREBASE_MESSAGING_SENDER_ID=votre_sender_id
FIREBASE_APP_ID=votre_app_id
FIREBASE_MEASUREMENT_ID=votre_measurement_id

# App Configuration
APP_NAME=TravelMate
APP_VERSION=1.0.0
APP_BUILD=1

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_LOCATION_TRACKING=true

# Cache Configuration
CACHE_TTL=3600
MAX_CACHE_SIZE=50

# Debug
ENABLE_LOGGING=true
ENABLE_CRASH_REPORTING=true
