{
  "expo": {
    "name": "TravelMate",
    "slug": "travelmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.travelmate.app"
    },
    "android": {
      "backgroundColor": "#ffffff",
      "package": "com.travelmate.app"
    },
    "plugins": [
      "expo-camera",
      "expo-location",
      "expo-image-picker"
    ]
  }
}