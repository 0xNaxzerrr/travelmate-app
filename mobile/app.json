{
  "expo": {
    "name": "TravelMate",
    "slug": "travelmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.travelmate.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.travelmate.app"
    },
    "plugins": [
      "expo-camera",
      "expo-location",
      "expo-image-picker",
      "@react-native-google-signin/google-signin"
    ]
  }
}