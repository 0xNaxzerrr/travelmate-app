{
  "expo": {
    "name": "TravelMate",
    "slug": "travelmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.travelmate.app",
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.travelmate.app",
      "newArchEnabled": true
    },
    "plugins": [
      "expo-camera",
      "expo-location",
      "expo-image-picker",
      "react-native-safe-area-context",
      "react-native-screens"
    ]
  }
}